package fileIO;

public class Student {

	private int rollno;
	private String name;
	private int marks;
	
	public Student() {
		
		rollno =01;
		name = new String("ABC");
		marks = 00;
	}
	
	public Student(int rollno,String name,int marks) {
		this.rollno = rollno;
		this.marks = marks;
		this.name = name;
		
	}

	public int getRollno() {
		return rollno;
	}

	public void setRollno(int rollno) {
		this.rollno = rollno;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getMarks() {
		return marks;
	}

	public void setMarks(int marks) {
		this.marks = marks;
	}
	
	
}
