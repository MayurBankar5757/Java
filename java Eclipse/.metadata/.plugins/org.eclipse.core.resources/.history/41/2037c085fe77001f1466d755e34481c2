package fileIO;

public class Student {

	private int rollno;
	private String name;
	private int marks;
	
	public Student() {
		
		rollno =01;
		name = new String("ABC");
		marks = 00;
	}
	
	public Student(int rollno,String name,int marks) {
		this.rollno = rollno;
		this.marks = marks;
		this.name = name;
		
	}
}
