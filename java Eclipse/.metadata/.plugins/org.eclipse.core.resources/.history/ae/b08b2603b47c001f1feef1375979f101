package Entity;

public class PrimeThread implements Runnable {

	private int start;
	private int end;
	
	public PrimeThread(int start,int end) {
		
		this.start = start;
		this.end = end;
		
	}

	@Override
	public void run() {
		
		for(int i=start ; i<end; i++) {
			
			if(i<0) {
			    System.out.println("Not prime number");
			}
			
			for(int j=2; j< i/2; j++) {
				
				if(i %j ==0) {
					System.out.println(i);
					break;
				}
			}
		}
		
		
		
	}
	
	
	
}
