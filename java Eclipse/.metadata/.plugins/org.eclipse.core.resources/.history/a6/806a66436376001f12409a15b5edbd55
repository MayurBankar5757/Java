package staff;

import office.utility.Emp;

public class Admin extends Emp {

	
		private double allowance;

		public Admin() {
			super();
			
		}

		public Admin(String name, int dd, int mm, int yy, int eId, double basicSalary) {
			super(name, dd, mm, yy, eId, basicSalary);
			
		}

		public Admin(double allowance) {
			super();
			this.allowance = allowance;
		}

		@Override
		public String toString() {
			return "Admin [allowance=" + allowance + ", dob=" + dob + ", geteId()=" + geteId() + ", getBasicSalary()="
					+ getBasicSalary() + ", toString()=" + super.toString() + ", getName()=" + getName()
					+ ", getClass()=" + getClass() + ", hashCode()=" + hashCode() + "]";
		}
		
		
		
}
